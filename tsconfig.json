{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["use-sync-external-store", "yargs"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./",
    "paths": {
      "@/components/*": ["./app/components/*"],
      "@/*": ["./*"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "dist/types/**/*.ts",
    "app/interfaces/**/*.ts",
    "middleware.ts",
    "node_modules/react-redux"
, "app/components/course-analytics/graphs/AvgCompletedQuizzes", "app/components/course-analytics/graphs/AvgCompletedQuizzes"  ],
  "exclude": ["node_modules"]
}
